{
  "name": "directorymonster",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "// APP COMMANDS": "Commands for running the application",
    "app:dev": "next dev",
    "app:dev:docker": "WATCHPACK_POLLING=true next dev --port 3000 --hostname 0.0.0.0",
    "app:build": "next build",
    "app:start": "next start",
    "app:lint": "next lint",
    "app:format": "prettier --write .",
    "app:typecheck": "tsc --noEmit",
    "app:all": "bash scripts/do-it-all.sh",
    "// DOCKER COMMANDS": "Commands for Docker operations",
    "docker:build": "docker-compose -f docker/docker-compose.yml build",
    "docker:build:python": "docker build -t directorymonster-python -f docker/Dockerfile.pythondev .",
    "docker:up": "docker-compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.yml down",
    "docker:logs": "docker-compose logs -f",
    "docker:all": "bash scripts/docker-do-it-all.sh",
    "docker:zkp:up": "docker-compose -f docker-compose.zkp.yml up -d",
    "docker:zkp:down": "docker-compose -f docker-compose.zkp.yml down",
    "docker:zkp:logs": "docker-compose -f docker-compose.zkp.yml logs -f",
    "// ZKP COMMANDS": "Commands for Zero-Knowledge Proof operations",
    "zkp:setup": "ts-node scripts/zkp-setup-simple.ts",
    "zkp:update": "node scripts/update-zkp-implementation.js",
    "zkp:debug": "ts-node scripts/debug-verification.ts",
    "zkp:test:poseidon": "ts-node scripts/test-poseidon-direct.ts",
    "zkp:test:passwords": "ts-node scripts/test-different-passwords.ts",
    "zkp:verify": "npm run test:crypto:setup",
    "// SECURITY COMMANDS": "Commands for security operations",
    "security:verify": "ts-node scripts/verify-checksums.ts",
    "security:generate": "ts-node scripts/generate-checksums.ts",
    "security:check": "npm run test:crypto:security && npm run security:verify",
    "security:audit": "npm audit --production",
    "security:all": "npm run test:crypto && npm run security:check && npm run security:audit",
    "// DATA COMMANDS": "Commands for data operations",
    "data:seed": "bash scripts/unix/run-seed.sh",
    "data:seed:js": "node seed-script.js",
    "data:seed:docker": "bash scripts/unix/run-seed-docker.sh",
    "data:seed:auth": "node scripts/seed-with-auth.js",
    "data:clear:users": "bash scripts/unix/clear-redis-users.sh",
    "// TEST COMMANDS": "Commands for testing",
    "test": "jest",
    "test:all": "jest",
    "test:all-security": "powershell -File scripts/test-all-security.ps1",
    "test:verify": "powershell -File scripts/verify-all.ps1",
    "test:unit": "jest --config jest.projects.config.js --selectProjects=UNIT",
    "test:failures": "jest --bail=false --testFailureExitCode=0 --reporters=./jest-detailed-reporter.js --testPathIgnorePatterns=\"src/components/admin/security/hooks/__tests__/|src/components/admin/notifications/hooks/__tests__/|tests/admin/integration/|tests/components/admin/security/|tests/hook-test-example.test.jsx|tests/admin/sites/|tests/integration/admin/users/|tests/unit/auth/|tests/crypto/|tests/lib/zkp\"",
    "test:fixed": "jest --testPathPattern=\"tests/ListingCard.test.tsx|tests/admin/dashboard/ActivityFeed.test.tsx|tests/middleware.test.ts|src/components/ui/__tests__/Button.test.tsx|src/components/ui/button/__tests__/Button.test.tsx|src/components/ui/__tests__/ButtonPresentation.test.tsx|src/components/ui/button/__tests__/Button.visual.test.tsx|src/components/ui/__tests__/DialogComponent.test.tsx|src/components/ui/__tests__/DialogContainer.test.tsx|src/components/ui/__tests__/DialogPresentation.test.tsx|src/components/ui/context-breadcrumbs/__tests__/ContextBreadcrumbs.test.tsx|src/components/ui/context-breadcrumbs/__tests__/BreadcrumbProvider.test.tsx|src/components/ui/keyboard-shortcut/__tests__/KeyboardShortcut.test.tsx|src/components/ui/advanced-search/__tests__/AdvancedSearchPresentation.test.tsx|src/components/ui/advanced-search/__tests__/AdvancedSearchPresentation.visual.test.tsx|src/components/ui/search/__tests__/SearchBar.test.tsx|src/components/search/__tests__/SearchResultsPresentation.test.tsx|src/components/search/hooks/__tests__/useSearchResults.test.ts\"",
    "test:integration": "jest --config jest.projects.config.js --selectProjects=INTEGRATION",
    "test:component": "jest --config jest.projects.config.js --selectProjects=COMPONENT",
    "test:hook": "jest --config jest.projects.config.js --selectProjects=HOOK",
    "test:api": "jest --config jest.projects.config.js --selectProjects=API",
    "test:failures:all": "jest --reporters=./jest-detailed-reporter.js",
    "test:failures:report": "node scripts/test-failures.js",
    "test:zkp": "cross-env ZKP_USE_MOCKS=true jest --config jest.config.zkp.js",
    "test:zkp:real": "cross-env ZKP_USE_MOCKS=false jest --config jest.config.zkp.js",
    "test:zkp:all": "node scripts/run-zkp-tests.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:critical-paths": "jest --config tests/integration/critical-paths/jest.config.js \"tests/integration/critical-paths/**/*.test.tsx\"",
    "test:components:categories": "jest \"tests/admin/categories/**/*.test.tsx\"",
    "test:components:listings": "jest \"tests/admin/listings/**/*.test.tsx\"",
    "test:refactored": "jest src/components/__tests__/MainHeader.test.tsx src/components/__tests__/MainHeaderContainer.test.tsx src/components/hooks/__tests__/useMainHeader.test.tsx src/components/ui/keyboard-shortcut/__tests__/KeyboardShortcut.test.tsx",
    "test:visual": "npx percy exec -- npx playwright test --config=percy.config.js",
    "test:visual:update": "npx percy exec -- npx playwright test --config=percy.config.js --update-snapshots",
    "test:user-management": "jest --config integration-tests.config.js",
    "test:crypto": "jest tests/crypto tests/lib/zkp-bcrypt.test.ts tests/lib/zkp-mock-adapter-bcrypt.test.ts tests/lib/rate-limit.test.ts tests/types/snarkjs.test.ts",
    "test:crypto:core": "jest tests/crypto/secure-zkp-cli.test.ts tests/crypto/simplified-zkp.test.ts tests/crypto/zkp-authentication.test.ts",
    "test:crypto:setup": "jest tests/crypto/zkp-setup-verification.test.ts",
    "test:crypto:security": "jest tests/crypto/zkp-security-measures.test.ts tests/crypto/zkp-privacy.test.ts tests/crypto/montgomery.test.ts tests/crypto/file-integrity.test.ts tests/crypto/poseidon-constants.test.ts tests/crypto/poseidon-rounds.test.ts tests/crypto/hash-truncation.test.ts tests/lib/zkp-bcrypt.test.ts tests/lib/zkp-mock-adapter-bcrypt.test.ts tests/lib/rate-limit.test.ts tests/types/snarkjs.test.ts",
    "test:e2e": "npm run data:clear:users && jest \"tests/e2e/first-user.test.js\" --testTimeout=60000 && jest \"tests/e2e/login.test.js\" \"tests/e2e/admin-dashboard.test.js\" \"tests/e2e/homepage.test.js\" \"tests/e2e/categories.test.js\" --testTimeout=60000",
    "test:e2e:all": "node tests/e2e/run-all-tests.js",
    "test:with:seed": "npm run data:seed && npm run test",
    "test:with:seed:docker": "npm run data:seed:docker && npm run test",
    "test:docker": "bash scripts/unix/docker-test-suite.sh",
    "test:no-e2e": "jest --testPathIgnorePatterns=\"/node_modules/|/.next/|/tests/e2e/\"",
    "// SIMPLE COMMANDS": "Simple commands for common operations",
    "run": "npm run app:dev",
    "build": "npm run app:build && npm run app:start",
    "test:security": "npm run test:all-security",
    "verify": "powershell -File scripts/verify-all.ps1",
    "docker": "powershell -File scripts/docker-do-it-all.ps1",
    "setup": "powershell -File scripts/do-it-all.ps1",
    "// WINDOWS COMMANDS": "Windows-specific commands",
    "win:test": "jest",
    "win:test:security": "powershell -File scripts/test-all-security.ps1",
    "win:verify": "powershell -File scripts/verify-all.ps1",
    "win:docker": "powershell -File scripts/docker-do-it-all.ps1",
    "win:setup": "powershell -File scripts/do-it-all.ps1"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-dialog": "^1.1.7",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/user-event": "^14.6.1",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "circomlib": "^2.0.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "ffjavascript": "^0.2.63",
    "glob": "^11.0.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.484.0",
    "mapbox-gl": "^3.11.0",
    "next": "^14.0.0",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-map-gl": "^8.0.2",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-mock-store": "^1.5.5",
    "snarkjs": "^0.7.3",
    "tailwind-merge": "^3.1.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@percy/cli": "^1.30.9",
    "@percy/playwright": "^1.0.7",
    "@playwright/test": "^1.51.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/bcrypt": "^5.0.2",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "eslint": "^8.52.0",
    "eslint-config-next": "^14.0.0",
    "handlebars": "^4.7.8",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-html-reporters": "^3.1.7",
    "jest-json-reporter": "^1.2.2",
    "jest-silent-reporter": "^0.6.0",
    "node-mocks-http": "^1.16.2",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "puppeteer": "^24.4.0",
    "supertest": "^6.3.4",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "overrides": {
    "tr46": "4.1.1",
    "whatwg-url": "13.0.0"
  }
}
