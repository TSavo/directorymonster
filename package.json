{
  "name": "directorymonster",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:integration": "jest \"tests/multitenancy-integration.test.ts\" \"tests/page-rendering.test.ts\"",
    "test:domain": "bash scripts/test-domain-resolution.sh",
    "test:multitenancy": "bash scripts/test-multitenancy-comprehensive.sh",
    "test:rendering": "bash scripts/test-comprehensive-rendering.sh",
    "test:all": "npm run test && npm run test:domain && npm run test:multitenancy && npm run test:rendering",
    "test:docker": "bash scripts/docker-test-suite.sh",
    "test:docker:dev": "bash scripts/docker-test-suite.sh --dev",
    "test:docker:prod": "bash scripts/docker-test-suite.sh --prod",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "docker:build": "docker-compose build",
    "docker:build:dev": "docker-compose -f docker-compose.dev.yml build",
    "docker:build:python": "docker build -t directorymonster-python -f Dockerfile.python .",
    "docker:up": "docker-compose up -d",
    "docker:up:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:down": "docker-compose down",
    "docker:down:dev": "docker-compose -f docker-compose.dev.yml down",
    "docker:logs": "docker-compose logs -f",
    "docker:logs:dev": "docker-compose -f docker-compose.dev.yml logs -f",
    "seed": "bash scripts/run-seed.sh",
    "seed:docker": "bash scripts/run-seed-docker.sh",
    "test:with-seed": "npm run seed && npm run test",
    "test:with-seed:docker": "npm run seed:docker && npm run test",
    "test:all-with-seed": "npm run seed && npm run test:all",
    "test:all-with-seed:docker": "npm run seed:docker && npm run test:all",
    "test:with-server": "bash scripts/run-tests-with-server.sh"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "ioredis": "^5.3.2",
    "next": "^14.0.0",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.52.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  }
}
